{
    "name": "Start on Heroku: Python",
    "description": "Friends app.",
    "image": "heroku/python",
    "repository": "https://github.com/julistary/W6-api-sentiment-project.git",
    "keywords": ["python", "friends" ],
    "addons": [ "heroku-postgresql" ],
    "env": {
      "SECRET_KEY": {
        "description": "The secret key for the application.",
        "generator": "secret"
      }
    },
    "environments": {
      "test": {
        "scripts": {
          "test-setup": "python manage.py collectstatic --noinput",
          "test": "python manage.py test"
        }
      }
    },
    "scripts": {
        "start": "node index.js"
     }
  }
  
